<Page x:Class="Confou.View.Frames.RegistrationFrame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Confou.View.Frames"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="250"
      Title="RegistrationFrame">

    <Grid Background="#FF0D1431">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Click="Button_Click" Content="Назад" Width="50" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10" Background="#FF373535" Foreground="White"/>
    </Grid>
          
</Page>
